modules = ["python-3.11"]
[nix]
channel = "stable-25_05"
packages = ["unzip"]

[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "TarimPazar"

[[workflows.workflow]]
name = "TarimPazar"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd TarimPazarzip/TarimPazar && python app.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 44043
externalPort = 3000
